// Tabuda de multiplicação a partir de entrada de usuário


import java.util.Scanner;

public class TabuadaDeVezes {

	public static void main(String[] args) {
		
		Scanner leia = new Scanner(System.in);
		
		int num, numfinal;
		
		System.out.println("Olá, digite um numero para obter sua tabuada:");
		num = leia.nextInt();
		
		System.out.println("Agora escreva até qual número quer que a tabuada vá, exemplo '100': ");
		numfinal = leia.nextInt();
		
		
		for (int i = 0; i <= numfinal; i++) {
			int results;
			results = i * num;
			System.out.println(results);
		}
		

	}

}
